<%- include('../partials/header') %>

<h1>My Surveys</h1>
<table>
    <thead>
        <tr>
            <th>Subject</th>
            <th>Case Number</th>
            <th>Comment</th>
            <th>Rating</th>
        </tr>
    </thead>
    <tbody>
        <% cases.forEach(c => { %>
            <% if(c.survey){ %>
                <tr>
                    <td><%= c.subject %></td>
                    <td><%= c.caseNum %></td>
                    <td><%= c.survey.comment %></td>
                    <td><%= c.survey.rating %></td>
                    <td><a href="/survey/<%= c._id %>/update">Edit</a></td>
                </tr>
            <% } %>
        <% }) %>
    </tbody>

<%- include('../partials/footer') %>